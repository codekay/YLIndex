<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.xu.dao.ServiceTypeDao">

    <select id="queryById" resultType="cn.xu.model.ServiceType">
        select * from message where id = #{id};
    </select>

    <select id="queryAll" resultType="cn.xu.model.ServiceType">
        select * from ServiceType;
    </select>

</mapper>